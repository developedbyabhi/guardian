{
    "name": "guardian/exception-tracker",
    "description": "trace exceptions of laravel projects",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Guardian\\ExceptionTracker\\": "src/"
        }
    },
    "keywords": [
        "sentry",
        "laravel",
        "log",
        "logging",
        "error-monitoring",
        "error-handler",
        "crash-reporting",
        "crash-reports",
        "profiling",
        "tracing"
    ],
    "authors": [
        {
            "name": "abhimanyu"
        }
    ],
    "require": {
        "php": "^7.2 | ^8.0",
        "illuminate/support": "^6.0 | ^7.0 | ^8.0 | ^9.0 | ^10.0 | ^11.0",
        "sentry/sentry": "^4.10",
        "symfony/psr-http-message-bridge": "^1.0 | ^2.0 | ^6.0 | ^7.0",
        "nyholm/psr7": "^1.0"
    },
    "minimum-stability": "dev", 
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^8.4 | ^9.3 | ^10.4",
        "laravel/framework": "^6.0 | ^7.0 | ^8.0 | ^9.0 | ^10.0 | ^11.0",
        "livewire/livewire": "^2.0 | ^3.0",
        "orchestra/testbench": "^4.7 | ^5.1 | ^6.0 | ^7.0 | ^8.0 | ^9.0",
        "friendsofphp/php-cs-fixer": "^3.11",
        "mockery/mockery": "^1.3",
        "phpstan/phpstan": "^1.10",
        "laravel/folio": "^1.1",
        "guzzlehttp/guzzle": "^7.2"
    }
}
